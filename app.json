{
  "name": "Maximus",
  "description": "[Maximus]Simple Fast Non Suspendable Userbot By @noobanon",
  "logo": "https://telegra.ph/file/5aee89bfdac1777d5f8f8.jpg",
  "keywords": [
    "telegram",
    "userbot",
    "plugin",
    "modular",
    "productivity"
  ],
  "repository": "https://github.com/noobanon/maximus",
  "website": "https://telegra.ph/Maximus-Userbot-04-27",
  "success_url": "https://t.me/TheBotSupport",
  "stack": "container",
  "env": {
    "API_KEY": {
      "description": "Get this value from my.telegram.org.",
      "required": true
    },
    "API_HASH": {
      "description": "Get this value from my.telegram.org.",
      "required": true
    },
    "STRING_SESSION": {
      "description": "Get this value in telegram using this @usetgxbot.",
      "required": true
    },
    
    "LOGGER": {
      "description": "Incase you want to turn off logging, put this to false",
      "value": "False"
    },
    "LOGGER_GROUP": {
      "description": "ChatID of your private group. Example: -1001368324312",
      "value": "0"
    },
    
    "SUDO_USERS": {
      "description": "Set the default city for the userbot's weather module.",
      "required": false
    },
    "PM_AUTO_BAN": {
      "description": "Use this value True if you want to block users after pm message extend",
      "value": "True"
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql",
      "options": {
        "version": "13"
      }
    }
  ]
}
